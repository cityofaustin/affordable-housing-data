{
  "name": "affordable-housing-project",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "server": "node ./bin/www",
    "start": "npm run server",

    "dev": "nodemon ./bin/www",
    "client:install": "cd client && npm install",
    "client:start": "cd client && npm start",
    "client:build": "cd client && npm build",
    "client:production": "cd client && REACT_APP_API_URL= npm run build",
    "start:production": "concurrently --kill-others-on-fail \"npm run client:build\" \"npm run server\"",
    "start-practice": "concurrently --kill-others-on-fail \"npm run client:build\" \"npm run server\"",
    "start:dev": "concurrently --kill-others-on-fail \"npm run client:start\" \"npm run dev\"",
    "local:production": "concurrently --kill-others-on-fail \"npm run client:build\" \"npm run server\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "sequelize": "^4.43.1",
    "underscore": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
